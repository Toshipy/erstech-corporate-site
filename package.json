  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev-https": "next dev --experimental-https",
    "build": "next build",
    "start": "next start",
    "start:debug": "NEXT_PRIVATE_DEBUG_CACHE=1 next start",
    "clean": "rimraf .next/cache/fetch-cache",
    "clean-dev": "run-s clean dev",
    "clean-dev-https": "run-s clean dev-https",
    "clean-build": "run-s clean build",
    "clean-start": "run-s clean build start:debug",
    "lint": "biome lint --write",
    "format": "biome format --write",
    "check": "biome check",
    "type-check": "tsc --noEmit",
    "all:check": "run-s type-check check lint",
    "all:fix": "biome check --write",
    "format:check": "prettier . --check --cache",
    "format:write": "prettier . --write --cache",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:serve": "npx http-server storybook-static -a localhost -p 6006 -s",
    "vrt:snapshot": "storycap --serverCmd \"npx http-server storybook-static -a localhost -p 6006\" http://localhost:6006",
    "vrt:expected": "run-s \"rimraf .reg/expected\" \"mv __screenshots__ .reg/expected\"",
    "vrt:run": "reg-suit run",
    "vrt:serve": "npx http-server .reg -o -p 8080",
    "test:jest": "jest",
    "test:update-jest-snapshot": "jest --updateSnapshot",
    "test:playwright": "playwright test",
    "knip": "knip"
